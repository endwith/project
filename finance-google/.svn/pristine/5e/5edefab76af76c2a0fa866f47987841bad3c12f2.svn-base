server.port= 8089
server.tomcat.uri-encoding= UTF-8
spring.http.encoding.force= true
spring.http.encoding.charset= UTF-8
spring.http.encoding.enabled= true

#数据库
spring.datasource.url= jdbc:mysql://localhost:3306/testdd?useUnicode=true&amp;characterEncoding=utf8
spring.datasource.username= root
spring.datasource.password= 123456
spring.datasource.driver-class-name=com.mysql.jdbc.Driver
spring.datasource.type=org.apache.commons.dbcp2.BasicDataSource
spring.datasource.dbcp2.initial-size= 50
spring.datasource.dbcp2.max-idle= 50
spring.datasource.dbcp2.min-idle=5
spring.datasource.dbcp2.max-wait-millis=10000
spring.datasource.dbcp2.validation-query= SELECT 1


## Mybatis
mybatis.type-aliases-package= com.longhang.model
mybatis.mapper-locations=classpath:mapper/*.xml