eureka:
  client:
    serviceUrl:
      defaultZone: http://39.108.173.252:20401/eureka/,http://120.78.91.61:20409/eureka/
server:
  port: 20413
spring:
  http:
    multipart:
      enabled: true   # 启用http上传处理
  application:
    name: service-zuul
zuul:
  routes:
    admin:
      path: /admin/**
      serviceId: server-feign-admin
      sensitiveHeaders:
    client:
      path: /client/**
      serviceId: server-feign-client
      sensitiveHeaders:
    uploadfile:
      path: /admin/**
      serviceId: server-feign-admin
  host:
    connect-timeout-millis: 10000
    socket-timeout-millis: 60000
  servlet-path: /
ribbon:
 ReadTimeout: 120000
 ConnectTimeout: 30000

feign:
  hystrix:
    enabled: true
hystrix:
  command:
    default:
      execution:
        isolation:
          thread:
            timeoutInMilliseconds: 60000