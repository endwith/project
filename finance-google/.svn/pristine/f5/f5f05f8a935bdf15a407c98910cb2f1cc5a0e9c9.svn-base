package com.ptteng.fuiou.h5.bean;

import javax.xml.bind.annotation.XmlElement;

public class H5WechatPay
{
	//版本号
	private String version;
	//商品描述, 商品或支付单简要描述
	private String goodsDes;
	//商品详情, 商品名称明细 
	private String goodsDetail;
	//商品标记
	private String goodsTag;
	//商品标识
	private String productId;
	//附加数据 
	private String addnInf;
	//通知地址, 接收富友异步通知回调地址，通知 url 必须为直可访问的 url，不能携带参数 主扫时必填
	private String notifyUrl;
	//用户标识 
	private String openid;
	//子商户公众号 id, trade_type 为JSAPI 时必传 
	private String appid;
	//终端IP
	private String termIp;
	
	private String homeurl;
	
	public String getVersion()
	{
		return version;
	}

	@XmlElement(name="VERSION")
	public void setVersion(String version)
	{
		this.version = version;
	}

	public String getGoodsDes()
	{
		return goodsDes;
	}
	@XmlElement(name="GOODS_DES")
	public void setGoodsDes(String goodsDes)
	{
		this.goodsDes = goodsDes;
	}

	public String getGoodsDetail()
	{
		return goodsDetail;
	}
	@XmlElement(name="GOODS_DETAIL")
	public void setGoodsDetail(String goodsDetail)
	{
		this.goodsDetail = goodsDetail;
	}

	public String getGoodsTag()
	{
		return goodsTag;
	}
	@XmlElement(name="GOODS_TAG")
	public void setGoodsTag(String goodsTag)
	{
		this.goodsTag = goodsTag;
	}

	public String getProductId()
	{
		return productId;
	}
	@XmlElement(name="PRODUCT_ID")
	public void setProductId(String productId)
	{
		this.productId = productId;
	}

	public String getAddnInf()
	{
		return addnInf;
	}
	@XmlElement(name="ADDN_INF")
	public void setAddnInf(String addnInf)
	{
		this.addnInf = addnInf;
	}

	public String getNotifyUrl()
	{
		return notifyUrl;
	}
	@XmlElement(name="NOTIFY_URL")
	public void setNotifyUrl(String notifyUrl)
	{
		this.notifyUrl = notifyUrl;
	}

	public String getOpenid()
	{
		return openid;
	}
	@XmlElement(name="OPENID")
	public void setOpenid(String openid)
	{
		this.openid = openid;
	}

	public String getAppid()
	{
		return appid;
	}

	@XmlElement(name="APPID")
	public void setAppid(String appid)
	{
		this.appid = appid;
	}

	public String getTermIp()
	{
		return termIp;
	}

	@XmlElement(name="TERM_IP")
	public void setTermIp(String termIp)
	{
		this.termIp = termIp;
	}

	public String getHomeurl()
	{
		return homeurl;
	}

	@XmlElement(name="HOMEURL")
	public void setHomeurl(String homeurl)
	{
		this.homeurl = homeurl;
	}
}
